<?xml version="1.0" encoding="utf-8"?>
<!-- Agregado para retroalimentar el toque del usuario -->
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_marginTop="10dp"
	android:paddingBottom="8dp"
	android:paddingTop="8dp"
	android:background="?android:attr/selectableItemBackground"
	android:id="@+id/rlSuscription">
	<!-- ModificaciÃ³n para aplicar redondeo de logos -->
	<de.hdodenhof.circleimageview.CircleImageView
		android:contentDescription="@string/app_name"
		android:id="@+id/circleView"
		android:layout_width="50dp"
		android:layout_height="50dp"
		android:layout_marginLeft="20dp"
		android:src="@drawable/ic_launcher"
		app:layout_behavior="com.tween.viacelular.utils.AvatarImageBehavior" />
	<TextView
		android:id="@+id/rowText"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:paddingLeft="10dp"
		android:text="@string/app_name"
		android:textColor="@color/black"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:layout_centerVertical="true"
		android:layout_toRightOf="@+id/circleView"
		android:layout_toEndOf="@+id/circleView" />
</RelativeLayout>